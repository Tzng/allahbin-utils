{
  "name": "allahbin-utils",
  "version": "1.0.0",
  "description": "A utility library for common JavaScript/TypeScript functions",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.js"
    },
    "./string": {
      "types": "./dist/string/index.d.ts",
      "import": "./dist/esm/string/index.js",
      "require": "./dist/string/index.js"
    },
    "./array": {
      "types": "./dist/array/index.d.ts",
      "import": "./dist/esm/array/index.js",
      "require": "./dist/array/index.js"
    },
    "./object": {
      "types": "./dist/object/index.d.ts",
      "import": "./dist/esm/object/index.js",
      "require": "./dist/object/index.js"
    },
    "./date": {
      "types": "./dist/date/index.d.ts",
      "import": "./dist/esm/date/index.js",
      "require": "./dist/date/index.js"
    },
    "./number": {
      "types": "./dist/number/index.d.ts",
      "import": "./dist/esm/number/index.js",
      "require": "./dist/number/index.js"
    },
    "./validation": {
      "types": "./dist/validation/index.d.ts",
      "import": "./dist/esm/validation/index.js",
      "require": "./dist/validation/index.js"
    },
    "./async": {
      "types": "./dist/async/index.d.ts",
      "import": "./dist/esm/async/index.js",
      "require": "./dist/async/index.js"
    },
    "./dom": {
      "types": "./dist/dom/index.d.ts",
      "import": "./dist/esm/dom/index.js",
      "require": "./dist/dom/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "utils",
    "utilities",
    "javascript",
    "typescript",
    "library"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/allahbin-utils.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/allahbin-utils/issues"
  },
  "homepage": "https://github.com/yourusername/allahbin-utils#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dayjs": "^1.11.18",
    "jsencrypt": "^3.5.4",
    "uuid": "^13.0.0"
  }
}